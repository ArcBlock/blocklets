{
  "name": "blocklets",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@arcblock/gatsby-theme-www": "^6.9.0",
    "@arcblock/ux": "^0.21.0",
    "@arcblock/www": "^6.7.0",
    "@material-ui/core": "^4.4.0",
    "@material-ui/icons": "^4.2.1",
    "@svgr/webpack": "^4.1.0",
    "axios": "^0.19.0",
    "flat": "^4.1.0",
    "gatsby": "^2.15.6",
    "gatsby-image": "^2.2.6",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "intl": "^1.2.5",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.7.2",
    "rehype-react": "^3.1.0",
    "remark-frontmatter": "^1.3.2",
    "styled-components": "^4.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "lint": "eslint src",
    "clean": "rm -rf public",
    "build:gatsby": "GATSBY_APP_DOMAIN=blocklet.arcblock.io gatsby build",
    "build": "npm run clean && npm-run-all build:*",
    "start:gatsby": "gatsby develop --host 0.0.0.0",
    "start": "npm-run-all --parallel start:*"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "husky": "^1.3.0",
    "lint-staged": "^8.1.0"
  }
}
